{
  "id": "builtin.eslint",
  "name": "ESLint",
  "description": "ESLint extension",
  "repository": "https://github.com/lvce-editor/eslint",
  "browser": "dist/eslintMain.js",
  "categories": [],
  "rpc": [
    "FileSystem.readFile",
    "FileSystem.readDirWithFileTypes",
    "FileSystem.stat"
  ],
  "activation": ["onDiagnostic:javascript", "onDiagnostic:typescript"],
  "languages": [
    {
      "id": "json",
      "fileNames": [".eslintrc"]
    },
    {
      "id": "ignore",
      "fileNames": [".eslintignore"]
    }
  ],
  "jsonValidation": [
    {
      "fileMatch": ".eslintrc",
      "url": "https://json.schemastore.org/eslintrc"
    },
    {
      "fileMatch": ".eslintrc.json",
      "url": "https://json.schemastore.org/eslintrc"
    }
  ]
}
